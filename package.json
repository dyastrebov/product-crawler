{
  "name": "product-crawler",
  "version": "0.0.1",
  "description": "Web crawler sample",
  "main": "index.js",
  "scripts": {
    "build": "npm run build --prefix=client && cd server && tsc",
    "start": "node server/build",
    "dev": "nodemon ./server/index.ts",
    "lint": "eslint . --quiet --fix",
    "format": "prettier --write .",
    "docker-build": "docker build -t product-crawler --network=host .",
    "docker-start": "docker volume create product-crawler-db && docker run --rm -p 5677:5677 -v product-crawler-db:/app/server/build/db product-crawler"
  },
  "nodemonConfig": {
    "ignore": [
      "client/*"
    ]
  },
  "author": "dyastrebov",
  "license": "ISC",
  "dependencies": {
    "cron": "^1.8.2",
    "express": "^4.17.1",
    "js-yaml": "^4.1.0",
    "jsdom": "^17.0.0",
    "node-fetch": "^2.6.1",
    "react": "^17.0.2",
    "react-debounce-input": "^3.2.4",
    "react-dom": "^17.0.2",
    "sqlite": "^4.0.23",
    "sqlite3": "^5.0.2",
    "swagger-ui-express": "^4.1.6",
    "tree-kill": "^1.2.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/cron": "^1.7.3",
    "@types/express": "^4.17.12",
    "@types/jest": "^26.0.15",
    "@types/jsdom": "^16.2.13",
    "@types/node": "^12.0.0",
    "@types/node-fetch": "^2.5.12",
    "@types/react": "^17.0.9",
    "@types/react-dom": "^17.0.0",
    "@types/react-redux": "^7.1.16",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "node-http-proxy": "^0.2.4",
    "node-sass": "^5.0.0",
    "nodemon": "^2.0.7",
    "prettier": "^2.3.1",
    "react-scripts": "4.0.3",
    "ts-node": "^10.0.0",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.3.2"
  }
}
